string fizzbuzz(num n) {
    bool fizz := (n % 3) == 0
    bool buzz := (n % 5) == 0
    if fizz and buzz {
        return "FizzBuzz"
    }
    if fizz {
        return "Fizz"
    }
    if buzz {
        return "Buzz"
    }
    return num_to_string(n)
}
print(fizzbuzz(2))
print(fizzbuzz(3))
print(fizzbuzz(5))
print(fizzbuzz(15))