string fizzbuzz(num n) {
    string result := ""
    num i := 0
    while i < n {
        i := i + 1
        bool fizz := (i % 3) == 0
        bool buzz := (i % 5) == 0
        if fizz {
            result := result + "Fizz"
        }
        if buzz {
            result := result + "Buzz"
        }
        if !fizz and !buzz {
            result := result + num_to_string(i)
        }
    }
    return result
}
print(fizzbuzz(100))
